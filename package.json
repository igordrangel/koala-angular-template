{
  "name": "koala-angular-template",
  "version": "11.2.18",
  "description": "Template angular using Angular and Material Angular",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4201",
    "build": "ng build ngx-koala && cd dist/ngx-koala",
    "build-doc": "ng build koala-template --prod --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "version": "git add -A projects",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && cd projects/ngx-koala && npm version patch"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.1.0",
    "@angular/cdk": "^11.1.0",
    "@angular/common": "~11.1.0",
