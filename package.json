    "@angular/core": "~11.1.0",
    "@angular/forms": "~11.1.0",
    "@angular/material": "^11.1.0",
    "@angular/platform-browser": "~11.1.0",
    "@angular/platform-browser-dynamic": "~11.1.0",
    "@angular/router": "~11.1.0",
    "angular-oauth2-oidc": "^10.0.3",
    "angular-oauth2-oidc-jwks": "^9.0.0",
    "exceljs": "^1.12.0",
    "file-saver": "^2.0.5",
    "jwt-decode": "^3.0.0",
    "jwt-encode": "^1.0.1",
    "koala-utils": "^1.0.16",
    "ng2-currency-mask": "^9.0.2",
    "ngx-mask": "^10.0.4",
    "ngx-papaparse": "^5.0.0",
    "rxjs": "~6.6.3",
    "stream": "0.0.2",
