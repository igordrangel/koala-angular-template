{
  "name": "koala-angular-template",
  "version": "1.0.170",
  "description": "Template angular using Angular and Material Angular",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "version": "git add -A projects",
    "postversion": "git push && git push --tags",
    "deploy-koala": "npm version patch && cd projects/ngx-koala && npm version patch && cd ../../ && ng build ngx-koala && cd dist/ngx-koala && npm publish --ignore-scripts",
    "deploy-multidesk-service": "npm version patch && cd projects/ngx-multidesk-service && npm version patch && cd ../../ && ng build ngx-multidesk-service && cd dist/ngx-multidesk-service && npm publish --ignore-scripts"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~10.1.4",
    "@angular/cdk": "^10.2.3",
    "@angular/common": "~10.1.4",
    "@angular/compiler": "~10.1.4",
    "@angular/core": "~10.1.4",
    "@angular/forms": "~10.1.4",
    "@angular/material": "^10.2.3",
    "@angular/platform-browser": "~10.1.4",
    "@angular/platform-browser-dynamic": "~10.1.4",
    "@angular/router": "~10.1.4",
    "jwt-decode": "^3.0.0",
    "jwt-encode": "^1.0.1",
    "ng2-currency-mask": "^9.0.2",
    "ngx-mask": "^10.0.4",
    "ngx-papaparse": "^5.0.0",
    "rxjs": "~6.6.3",
    "tskoala-helpers": "^1.0.8",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.4",
    "@angular-devkit/build-ng-packagr": "~0.1001.4",
    "@angular/cli": "~10.1.4",
    "@angular/compiler-cli": "~10.1.4",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.7"
  }
}
