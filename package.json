{
  "name": "koala-angular-template",
  "version": "11.0.10",
  "description": "Template angular using Angular and Material Angular",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "version": "git add -A projects",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch && cd projects/ngx-koala && npm version patch && cd ../../ && ng build ngx-koala && cd dist/ngx-koala && npm publish --ignore-scripts",
    "deploy-break-version": "ng build ngx-koala && cd dist/ngx-koala && npm publish --ignore-scripts"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.0.5",
    "@angular/compiler": "~11.0.5",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~11.0.5",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~11.0.5",
    "@angular/platform-browser-dynamic": "~11.0.5",
    "@angular/router": "~11.0.5",
    "json-as-xlsx": "^1.1.8",
    "jwt-decode": "^3.0.0",
    "jwt-encode": "^1.0.1",
    "koala-utils": "^1.0.16",
    "ng2-currency-mask": "^9.0.2",
    "ngx-mask": "^10.0.4",
    "ngx-papaparse": "^5.0.0",
    "rxjs": "~6.6.3",
    "tskoala-helpers": "^1.0.8",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.5",
    "@angular-devkit/core": "^11.0.5",
    "@angular/cli": "~11.0.5",
    "@angular/compiler-cli": "~11.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
