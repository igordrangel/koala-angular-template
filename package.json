{
  "name": "koala-angular-template",
  "version": "12.7.14",
  "description": "Koala UI is a Design System developed in Angular whose objective is to facilitate and make your development faster and simpler, making this framework your greatest ally.",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node scripts/publish.js && npm run format",
    "build-doc": "ng build koala-template --configuration production --build-optimizer",
    "format": "prettier --write \"dist/package.json\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc",
    "version": "git add -A projects",
    "postversion": "git push && git push --tags",
    "deploy": "npm version patch",
    "analize-bundle": "ng build koala-template --stats-json && webpack-bundle-analyzer dist/koala-template/stats.json"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igordrangel/koala-angular-template.git"
  },
  "keywords": [
    "koala",
    "angular material",
    "angular template",
    "design system"
  ],
  "author": "Igor D. Rangel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/igordrangel/koala-angular-template/issues"
  },
  "homepage": "https://github.com/igordrangel/koala-angular-template#readme",
  "dependencies": {
    "@angular/animations": "~12.0.3",
    "@angular/cdk": "^12.0.3",
    "@angular/common": "~12.0.3",
    "@angular/compiler": "~12.0.3",
    "@angular/core": "~12.0.3",
    "@angular/forms": "~12.0.3",
    "@angular/material": "^12.0.3",
    "@angular/platform-browser": "~12.0.3",
    "@angular/platform-browser-dynamic": "~12.0.3",
    "@angular/router": "~12.0.3",
    "@koalarx/utils": "^1.0.88",
    "exceljs": "^3.10.0",
    "file-saver": "^2.0.5",
    "jwt-decode": "^3.0.0",
    "jwt-encode": "^1.0.1",
    "ng2-currency-mask": "^9.0.2",
    "ngx-device-detector": "^2.1.1",
    "ngx-mask": "^10.0.4",
    "ngx-papaparse": "^5.0.0",
    "rxjs": "~6.6.3",
    "stream": "0.0.2",
    "tslib": "^2.2.0",
    "xlsx": "^0.17.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.3",
    "@angular-devkit/core": "^12.0.3",
    "@angular/cli": "~12.0.3",
    "@angular/compiler-cli": "~12.0.3",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.0.3",
    "postcss": "^8.2.4",
    "prettier": "^2.4.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.2.4"
  }
